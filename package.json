{
  "packageManager": "npm@10.1.0",
  "name": "@jinx/threejs",
  "version": "1.0.0",
  "description": "This is mono-repo for learning three-js",
  "author": "kautilya",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev:camera": "turbo run dev --filter=@jinx/camera",
    "dev:scene": "turbo run dev --filter=@jinx/scene",
    "dev:mesh" : "turbo run dev --filter=@jinx/mesh",
    "turbo-build": "turbo run build",
    "turbo-force-build": "turbo run build --force",
    "turbo-dry-build": "turbo run build --dry"

  },
  "workspaces": [
    "./apps/*"
  ],
  "dependencies": {
    "@react-three/drei": "^9.112.0",
    "@react-three/fiber": "8.16.8",
    "@types/three": "0.167.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-scripts": "5.0.1",
    "three": "0.167.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "customize-cra": "1.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "leva": "0.9.35",
    "react-app-rewired": "2.2.1",
    "turbo": "2.0.11"
  },
  "overrides": {
    "leva": {
      "@radix-ui/react-portal": "1.0.2"
    }
  },
  "homepage": "https://github.com/harikautilya/jinx-three-js#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/harikautilya/jinx-three-js.git"
  },
  "keywords": [
    "threejs",
    "reactjs"
  ],
  "bugs": {
    "url": "https://github.com/harikautilya/jinx-three-js/issues"
  }
}
